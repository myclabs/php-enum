includes:
	- phpstan-baseline.neon

parameters:
    level: 7
    reportUnmatchedIgnoredErrors: false

    paths:
        - src
        - static-analysis
        - stubs
        - tests
        - bin/dev/php-cs-fixer-update-base-line
        - .php-cs-fixer.dist.php
        - .php-cs-fixer-finder.php

    parallel:
        processTimeout: 300.0